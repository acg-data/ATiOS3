{
  "expo": {
    "name": "Ace Ticket",
    "slug": "aceticket-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aceticket.ios",
      "infoPlist": {
        "NSCalendarsUsageDescription": "Ace Ticket needs access to add events to your calendar",
        "NSPhotoLibraryUsageDescription": "Ace Ticket needs access to update your profile photo"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.aceticket.android",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Ace Ticket to use your location to show nearby venues."
        }
      ],
      ["expo-calendar"],
      ["expo-image-picker"]
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}